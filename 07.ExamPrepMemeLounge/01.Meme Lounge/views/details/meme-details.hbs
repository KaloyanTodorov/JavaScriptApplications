
<div class="meme-details">
    <div class="my-meme-details">
        <a href="#" id="meme-title">{{title}}</a>
        <img src="{{imageUrl}}">
        <div class="meme-props">
            <h2>Description</h2>
            <p class="meme-description">
                {{description}}
            </p>
        </div>

        <div class="meme-details-buttons">
            <a class="meme-details-button" href="#/profile/{{userId}}">Created by User</a>
            <a href="#/edit{{id}}" class="meme-details-button">Edit</a>
            <a href="#/delete{{id}}" class="meme-details-button">Delete</a>
        </div>


    </div>
</div>